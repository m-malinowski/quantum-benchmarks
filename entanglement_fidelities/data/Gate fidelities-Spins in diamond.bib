month = {nov},
number = {1},
pages = {1--7},
publisher = {Nature Publishing Group},
title = {{Experimental fault-tolerant universal quantum gates with solid-state spins under ambient conditions}},
url = {www.nature.com/naturecommunications},
volume = {6},
year = {2015}
}
