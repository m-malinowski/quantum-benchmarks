author = {Petit, L. and Eenink, H. G.J. and Russ, M. and Lawrie, W. I.L. and Hendrickx, N. W. and Philips, S. G.J. and Clarke, J. S. and Vandersypen, L. M.K. and Veldhorst, M.},
doi = {10.1038/s41586-020-2170-7},
